{
    "version":2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node"}
    ],
    "routes": [
        { "src": "/register", "dest": "/" },
        { "src": "/login", "dest": "/" },
        { "src": "/home", "dest": "/" },
        { "src": "/users", "dest": "/" },
        { "src": "/(.*)", "dest": "/" }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/" }
    ]
}